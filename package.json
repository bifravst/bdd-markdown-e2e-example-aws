{
  "name": "@nordicsemiconductor/e2e-bdd-test-runner-example",
  "version": "0.0.0-development",
  "description": "Example use of @nordicsemiconductor/bdd-markdown",
  "type": "module",
  "scripts": {
    "test": "$(set -o pipefail && npx tsx features/run-features.ts | npx tsx features/console-reporter.ts)",
    "test:unit": "npx tsx --test ./features/*.spec.ts",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js.git"
  },
  "keywords": [
    "bdd",
    "aws",
    "cloudnative",
    "e2e",
    "bdd",
    "testing"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/cloud-bdd-markdown-e2e-example-aws-js#readme",
  "devDependencies": {
    "@aws-lambda-powertools/tracer": "2.1.0",
    "@aws-sdk/client-cloudformation": "3.569.0",
    "@aws-sdk/client-sqs": "3.569.0",
    "@aws-sdk/client-xray": "3.569.0",
    "@bifravst/eslint-config-typescript": "6.0.25",
    "@bifravst/prettier-config": "1.0.0",
    "@commitlint/config-conventional": "19.2.2",
    "@nordicsemiconductor/bdd-markdown": "8.1.10",
    "@nordicsemiconductor/cloudformation-helpers": "9.0.3",
    "@sinclair/typebox": "0.32.28",
    "@swc/core": "1.5.2",
    "@types/aws-lambda": "8.10.137",
    "@types/glob": "8.1.0",
    "@types/node": "20.12.8",
    "@types/yazl": "2.4.5",
    "aws-cdk-lib": "2.140.0",
    "cdk": "2.140.0",
    "constructs": "10.3.0",
    "dependency-tree": "11.0.1",
    "glob": "10.3.12",
    "husky": "9.0.11",
    "node-fetch": "3.3.2",
    "tsx": "4.9.1",
    "yazl": "2.5.1"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint"
    ],
    "*.{md,json,yaml,yml,js}": [
      "prettier --write"
    ]
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "prettier": "@bifravst/prettier-config"
}
